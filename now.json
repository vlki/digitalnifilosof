{
    "version": 2,
    "name": "digitalnifilosof",
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "public" }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "status": 404, "dest": "/404.html" }
    ]
  }
